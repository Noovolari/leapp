    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.2.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-firefox-launcher": "^2.1.0",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "~1.5.4",
    "karma-requirejs": "^1.1.0",
    "ng-packagr": "15.2.2",
    "node-abi": "^2.26.0",
    "nyc": "^15.1.0",
    "protractor": "^7.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.9.0",
    "util": "^0.12.4"
  }
}